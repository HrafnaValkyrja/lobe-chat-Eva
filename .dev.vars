# Local development environment variables for Wrangler
# Copy your .env.local variables here and add Cloudflare-specific ones

# Database configuration
DATABASE_DRIVER=d1
NEXT_PUBLIC_SERVICE_MODE=server

# Encryption key (generate with: openssl rand -base64 32)
KEY_VAULTS_SECRET=your-32-char-base64-encryption-key-here

# AI Provider API Keys (add your actual keys)
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
AZURE_OPENAI_API_KEY=your-azure-openai-api-key
GOOGLE_API_KEY=your-google-api-key

# Discord Bot Configuration (for future integration)
DISCORD_BOT_TOKEN=your-discord-bot-token
DISCORD_CLIENT_ID=your-discord-client-id
DISCORD_CLIENT_SECRET=your-discord-client-secret

# Other optional configurations
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=
